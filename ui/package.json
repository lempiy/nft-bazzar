        "start": "yarn clean && parcel src/index.html",
        "fmt": "prettier --write '{*,**/*}.{js,ts,jsx,tsx,json}'",
        "lint": "eslint . && prettier --check '{*,**/*}.{js,ts,jsx,tsx,json}'",
        "lint:fix": "eslint --fix . && yarn fmt",
        "nuke": "shx rm -rf node_modules yarn.lock"
    },
